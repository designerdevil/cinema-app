{
    "name": "cinema-experience",
    "version": "0.0.2",
    "description": "React Redux Express Webpack universal app..",
    "repository": "",
    "main": "index.js",
    "scripts": {
        "serve": "node compiled/server.js",
        "prepush": "npm run test",
        "lint": "npm run lint:js && npm run lint:scss",
        "lint:js": "eslint --quiet --ext .js,.jsx app server",
        "lint:scss": "stylelint \"**/*.scss\"",
        "lint:fix": "npm run lint:js:fix && npm run lint:scss:fix",
        "lint:js:fix": "eslint --quiet --ext .js,.jsx app server --fix",
        "lint:scss:fix": "stylelint \"**/*.scss\" --fix",
        "clean": "rimraf public && rimraf compiled",
        "start": "npm run build && cross-env NODE_ENV=production node compiled/server.js",
        "dev": "npm run copy && npm run copy-js && cross-env NODE_ENV=development npm run webpack --env.browser && cross-env NODE_ENV=development nodemon",
        "dev:debug": "npm run copy && npm run copy-js && cross-env NODE_ENV=development npm run webpack --env.browser && cross-env NODE_ENV=development DEBUG=cinema* nodemon",
        "build:dev": "cross-env NODE_ENV=development npm run webpack -- --env.server --mode development",
        "build": "npm run clean && npm run copy && npm run copy-js && cross-env NODE_ENV=production npm run webpack -- --env.browser && cross-env NODE_ENV=production npm run webpack -- --env.server",
        "webpack": "webpack --colors --display-error-details --config ./webpack/webpack.config.js",
        "test": "jest --bail --verbose ",
        "test:watch": "jest --bail --watch --verbose --coverage",
        "test:usnap": "jest --updateSnapshot",
        "test:cov": "jest --coverage",
        "copy": "copyfiles -u 2 ./app/images/**/*.* ./compiled/images && copyfiles -u 2 ./app/images/**/*.* ./public/assets/images && copyfiles -f ./app/fonts/*.* ./compiled/fonts && copyfiles -f ./app/fonts/*.* ./public/assets/fonts",
        "copy-js": "copyfiles -f ./app/third-party/*.* ./public/assets/third-party",
        "prettier": "prettier  --single-quote --trailing-comma es5 --write '{app,server}/**/*.{js,jsx,json,scss}'",
        "format": "prettier-eslint --write '{app,server}/**/*.{js,jsx,json,scss}'",
        "precommit": "lint-staged",
        "create": "babel-node ./webpack/automation/index.js"
    },
    "lint-staged": {
        "*.{js,jsx,json,scss}": [
            "prettier-eslint --write",
            "git add"
        ]
    },
    "author": "Sapient",
    "license": "MIT",
    "devDependencies": {
        "autoprefixer": "^8.1.0",
        "babel": "^6.23.0",
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.0",
        "babel-eslint": "^8.2.2",
        "babel-loader": "^7.1.4",
        "babel-plugin-react-transform": "^3.0.0",
        "babel-plugin-transform-react-constant-elements": "^6.23.0",
        "babel-plugin-transform-react-inline-elements": "^6.22.0",
        "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
        "babel-preset-env": "^1.6.1",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-node6": "^11.0.0",
        "babel-preset-react": "^6.24.1",
        "babel-preset-react-hmre": "^1.1.1",
        "babel-preset-stage-0": "^6.24.1",
        "babel-preset-stage-2": "^6.24.1",
        "babel-register": "^6.24.0",
        "bootstrap": "^4.0.0",
        "copyfiles": "^1.2.0",
        "css-loader": "^0.28.7",
        "enzyme": "^3.3.0",
        "enzyme-adapter-react-16": "^1.1.1",
        "eslint": "4.18.2",
        "eslint-config-airbnb": "^16.1.0",
        "eslint-config-airbnb-base": "^12.1.0",
        "eslint-config-prettier": "^2.9.0",
        "eslint-plugin-import": "^2.9.0",
        "eslint-plugin-jsx-a11y": "^6.0.3",
        "eslint-plugin-react": "^7.7.0",
        "extract-text-webpack-plugin": "^4.0.0-beta.0",
        "file-loader": "^0.11.2",
        "husky": "^0.14.3",
        "jest": "^22.4.2",
        "jsdom": "^11.6.2",
        "jshint": "^2.9.5",
        "jslint": "^0.11.0",
        "json-loader": "^0.5.4",
        "lint-staged": "^7.0.0",
        "node-sass": "^4.5.3",
        "nodemon": "^1.10.2",
        "npm": "^5.4.2",
        "npm-run-all": "^4.1.1",
        "optimize-css-assets-webpack-plugin": "^4.0.0",
        "postcss-cssnext": "^3.1.0",
        "postcss-import": "^11.1.0",
        "postcss-loader": "^2.1.1",
        "postcss-reporter": "^5.0.0",
        "prettier-eslint-cli": "^4.7.1",
        "randomstring": "^1.1.5",
        "react-test-renderer": "^15.5.3",
