        "sass-loader": "^6.0.6",
        "style-loader": "^0.16.1",
        "stylelint": "^9.1.1",
        "stylelint-config-standard": "^18.2.0",
        "stylelint-webpack-plugin": "^0.9.0",
        "uglifyjs-webpack-plugin": "^1.2.2",
        "url-loader": "^1.0.1",
        "webpack": "^4.0.0",
        "webpack-cli": "^3.1.1",
        "webpack-dev-middleware": "^3.0.1",
        "webpack-hot-middleware": "^2.18.0",
        "webpack-manifest-plugin": "^2.0.0-rc.2"
    },
    "engines": {
        "node": ">=6.x"
    },
    "dependencies": {
        "app-root-dir": "^1.0.2",
        "axios": "^0.17.1",
        "body-parser": "^1.17.1",
        "classnames": "^2.2.3",
        "compression": "^1.6.2",
        "cross-env": "^4.0.0",
        "debug": "^3.1.0",
        "es6-promise": "^4.1.0",
        "express": "^4.16.1",
        "express-flash": "0.0.2",
        "express-session": "^1.15.6",
        "fs": "0.0.1-security",
        "fs-extra": "^4.0.2",
        "glob": "^7.1.2",
        "helmet": "^3.12.0",
        "html-to-react": "^1.2.11",
        "inquirer": "^3.3.0",
        "install": "^0.10.1",
        "lodash": "^4.16.2",
        "method-override": "^2.3.8",
        "passport": "^0.3.2",
        "passport-google-oauth": "^1.0.0",
        "passport-local": "^1.0.0",
        "path": "^0.12.7",
        "prop-types": "^15.5.4",
        "qs": "^6.5.1",
        "react": "^16.2.0",
        "react-addons-css-transition-group": "^15.6.0",
        "react-addons-shallow-compare": "^15.6.0",
